  "main": "index.js",
  "scripts": {
    "develop:js": "esbuild src/p-ajax.ts --bundle --watch --outfile=dist/p-ajax.js",
    "develop:sw": "esbuild src/service-worker/service-worker.ts --bundle --watch --outfile=dist/p-ajax-sw.js",
    "develop": "concurrently \"npm:develop:js\" \"npm:develop:sw\" \"npm:serve\"",
